        <activity
            android:name="wubbalubbadubdub.hw1700ransomware.SelectUserActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="wubbalubbadubdub.hw1700ransomware.AddEventActivity"
            android:screenOrientation="portrait" />
